    <record model="ir.ui.view" id="view_notif_listado_tree">
        <field name="name">Listado de Notificaciones</field>
        <field name="model">pyme.notificaciones</field>
        <field name="arch" type="xml">
            <tree string="Listado Notificaciones" default_order='create_date desc'>
					<field name="create_date" string="Fecha Registro"/> 
					<field name="tiponotificaicon" invisible="1"/> 
					<field name="destino"/>	
					<field name="name" />
					<field name="modelid"/>
