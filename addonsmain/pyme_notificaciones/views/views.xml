    <record model="ir.ui.view" id="view_notif_conf_base_tree">
        <field name="name">Configuración de Notificaciones</field>
        <field name="model">pyme.notificaciones.configuracion</field>
        <field name="arch" type="xml">
            <tree string="Configuracion Notificaciones" default_order='create_date desc'>
                    <field name="sequence" widget="handle"/>
					<field name="name"/> 
					<field name="tipodestino"/>
					<field name="cuandonotifico"/>
					<field name="plantillausuario"/>
					<field name="enviabot"/>
					<field name="enviamail"/>
					<field name="envianotifinterna" attrs="{'invisible': [('tipodestino', '!=', 'users')]}"/>
            </tree>
        </field>
    </record>


    <record model="ir.ui.view" id="view_notif_conf_base_search">
        <field name="name">Configuración de Notificaciones</field>
        <field name="model">pyme.notificaciones.configuracion</field>
        <field name="arch" type="xml">
        <search string="Buscador de Configuraciones">
		    <field name="name"/>
			<field name="tipodestino"/>
			<field name="cuandonotifico"/>
			
		</search>
        </field>
    </record>

    <record model="ir.ui.view" id="view_notif_conf_base_form">
        <field name="name">Configuración de Notificaciones</field>
        <field name="model">pyme.notificaciones.configuracion</field>
        <field name="arch" type="xml">
            <form>
			<sheet>
				  <div class="alert alert-info" role="alert" style="height: 60px;margin-bottom:0px;">
                     <center><bold><h2>DEBES TENER EN CUENTA QUE SI UN CONTACTO NO TIENE HABILITADAS LAS NOTIFICACIONES NO SE LE NOTIFICARÁ</h2></bold></center>
                   </div>
				   <br/>
			 <group><group>
                    <field name="create_date" string="Fecha Registro"/> 
					<field name="name" options="{'no_create': True}"/>
					<field name="namemodel" invisible="1"/>
					<field name="tipodestino" required="1"/>					
					<field name="cuandonotifico" options="{'no_create': True}"/>

					</group><group>
					<label for="enviabot"/>
					<div>
					  <field name="enviabot" class="oe_inline"/><label for="enviabot" string=" " class="oe_inline"/>
					  <field name="botutilizado" class="oe_inline" widget="selection" attrs="{'invisible': [('enviabot', '!=', True)],'required': [('enviabot', '=', True)]}" options="{'no_create': True}"/>
					</div>
					<label for="enviamail" attrs="{'invisible': [('namemodel', '=', 'fetchmail.server')]}"/>
					<div  attrs="{'invisible': [('namemodel', '=', 'fetchmail.server')]}">
					  <field name="enviamail" class="oe_inline"/><label for="enviamail" string=" " class="oe_inline"/>
					  <field name="mailutilizado" class="oe_inline" widget="selection" attrs="{'invisible': [('enviamail', '!=', True)],'required': [('enviamail', '=', True)]}" options="{'no_create': True}"/>
					</div>
					<field name="envianotifinterna" attrs="{'invisible': [('tipodestino', '!=', 'users')]}"/>
					<!-- <field name="establecehorario" help="Si se marca esta casilla debes especificar un horario en las Pestañas de Abajo"/> -->
					
				</group>
            </group>            
            <div attrs="{'invisible': [('namemodel', '!=', 'fetchmail.server')]}">
             <group string="Opciones Aviso Correo Entrante">
			   <field name="emailorigendes" colspan="3"/>
			 </group>
            </div>
            <div attrs="{'invisible': [('namemodel', '!=', 'mail.channel')]}">
             <group string="Opciones Aviso Conversaciones">
			   <field name="convorigendes" colspan="3"/>
			 </group>
            </div>
            <div attrs="{'invisible': [('namemodel', '!=', 'helpdesk_lite.ticket')]}">
			<group string="Gestión Notificaciones Tickets / HelpDesk" attrs="{'invisible': [('tipodestino', '!=', 'users')]}">
            <group>					
			        <field name="notificaadmin" colspan="3"/>
					<field name="notificagroup" colspan="3"/>
					<field name="notificaasignado" colspan="3"/>
					</group><group>
					<h3>En caso de NO haber Usuario Asignado al Documento, este Notificará al Grupo y al Responsable</h3>
            </group></group>
			</div> 

			
			<notebook>
			<page string="Plantillas">
			<group>
			<field name="plantillausuario" required="1"/>
			</group>
            <group string="Ayuda">
			<div>
			  <b>$id</b> :: Devuelve el ID de Documento<br/>
			  <b>$fecha</b> :: Devuelve la fecha de la alerta<br/>
			  <b>$asunto</b> :: Devuelve el Tema/Asunto del Documento<br/>
			  <b>$from</b> :: Devuelve el Origen en caso de ser Correo Electrónico<br/>
			  <b>$usuario</b> : Devuelve el nombre del Usuario incluido en la alerta<br/>
			  <b>$cliente</b> : Devuelve el nombre del Cliente incluido en la alerta<br/>
			  <b>$estado</b> : Devuelve el Estado del documento incluido en la alerta<br/>
			  <b>$asignado</b> : Devuelve el nombre del Usuario asignado en la alerta<br/>
			  <b>$mensaje</b> :: Devuelve el mensaje enviado<br/>
            </div>
			</group>
			</page>
			<page string="Telegram">
			 <group>
			  <field name="botonestelgram">
			  <tree editable="top">
			   <field name="name"/>
			   <field name="tipo"/>
			   <field name="datos"/>
			  </tree>
			 </field>

			<group>
           
			<div>
			 <b><h2>Variables</h2></b><br/>
			 <b>$respid</b> - ID de esta respuesta.<br/>
			 <b>$idcliente</b> - ID del Contacto.<br/>
			 <b>$cliente</b> - Nombre del Contacto.<br/>
			 <b>$chatid</b> - Id del Chat (TelegramID o Conversación).<br/>
			 <b>$docid</b> - Id del Documento (Ticket, Tarea...)<br/>
             <br/>
			</div>

			<div>
			<b><h2>Funciones</h2></b><br/>
			<b>registro-paso1|$chatid</b> - Inicia el Registro.<br/>
			<b>ver-mis-tickets|$idcliente</b> - Mostrar Tickets.<br/>
			<b>ver-doc|$idcliente|$docid|[MODELO]</b> - Mostrar 1 Documento.<br/><br/>
			- [MODELO] : helpdesk_lite.ticket, project.task

			</div>
			
			
			</group>
			</group>
			</page>
			
			
			<page string="Alertas Utilizadas">
			<field name="alertasnotif" readonly="1">
			<tree>
               <field name="create_date"/>
			   <field name="name"/>
			</tree>
			</field>
			</page>
			</notebook>
			</sheet>
			<div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers" groups="base.group_user"/>
                    <field name="activity_ids" widget="mail_activity"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
			</form>
        </field>
    </record>
	

    <record model="ir.ui.view" id="view_notif_conf_cuando_tree">
