<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="im_livechat_channel_tipoin_view_form" model="ir.ui.view">
    <field name="name">im_livechat_channel_tipoin_view_form</field>
    <field name="model">im_livechat.channel</field>
    <field name="inherit_id" ref="im_livechat_rasa_bot.im_livechat_channel_rasa_view_form"/>
    <field name="arch" type="xml">
      <field name="bot" position="replace">
		<field name="bot" readonly="1" string="Bot" options="{'no_create': True}" domain="[('tipochatbot','=','odoobot')]" attrs="{'invisible': [('rasabot_online', '!=', True)]}"/>
	    <field name="tipochatbot" invisible="1"/>
      </field>
    </field>
  </record>
  </odoo>
  
  
