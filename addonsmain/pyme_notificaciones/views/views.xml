    <record model="ir.ui.view" id="view_notif_conf_form">
        <field name="name">Listado de Notificaciones</field>
        <field name="model">pyme.notificaciones</field>
        <field name="arch" type="xml">
            <form string="Configuracion Notificaciones">
			<sheet>
				<group>
                   <group string="Configuración General">
					<field name="tiponotificaicon" readonly="1" invisible="1"/> 
					<field name="destino" readonly="1"/>
					<field name="name" />
					<field name="modo" invisible="1"/>
					<field name="modelid" options="{'no_create': True}"/>
					</group>
                    <group>	
                    <group string="Estado">
					<!-- <button string="Envia BOT" name="enviabot" type="object"/> -->
					 <field name="create_date" string="Fecha Registro" readonly="1"/> 
					 <field name="enviado" readonly="1"/>
					 <field name="pendiente" readonly="1"/>
					 <field name="quienfh" readonly="1"/>
					 <field name="idconfnotif" readonly="1"/>
					 <field name="enviado_cuando" readonly="1" attrs="{'invisible': [('enviado', '!=', True)]}"/>

                    </group>					
					</group>
               </group>
			    <notebook>
                <page string="Plantillas">
				 <group>
				 <group >
				  <field name="mensaje_usuario"/>
				 </group>
				 </group>
			    </page>
                <page string="Destinos">
                  <group>
				  <label for="enviadotelegram"/>
				  <div>
				  <field name="enviadotelegram" class="oe_inline"/>
				  <field name="tenviadotelegram" class="oe_inline"/>
				  </div>
				  <label for="enviadomail"/>
				  <div>
				  <field name="enviadomail" class="oe_inline"/>
				  <field name="tenviadomail" class="oe_inline"/>
				  </div>
				  <label for="enviadobot" attrs="{'invisible': [('destino', '!=', 'users')]}"/>
				  <div>
				  <field name="enviadobot" class="oe_inline" attrs="{'invisible': [('destino', '!=', 'users')]}"/>
				  <field name="tenviadobot" class="oe_inline" attrs="{'invisible': [('destino', '!=', 'users')]}"/>
				  </div>



				  </group>

				  <field name="user_ids" attrs="{'invisible': [('destino', '!=', 'users')]}">
				  <tree>
				   <field name="name"/>
				   <field name="horario"/>
	  			  </tree>
				  </field>
				  <field name="partner_ids" attrs="{'invisible': [('destino', '!=', 'partner')]}">
				  <tree>
				   <field name="name"/>
				   <field name="horario"/>
	  			  </tree>

				  </field>



			    </page>

                    </notebook>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers" groups="base.group_user"/>
                    <field name="activity_ids" widget="mail_activity"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
			   
			   
            </form>
        </field>
    </record>

