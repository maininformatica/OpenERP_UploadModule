<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_res_partner_inherit_extra_data1_form" model="ir.ui.view">
        <field name="name">view.res.partner.inherit.extra.data1.form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
          <xpath expr="//page[1]" position="after">
          <page string="Extra Partner">
           <group>
		   <group string="LocalizaciÃ³n">
             <field name="latitud"/>
             <field name="longitud"/>
             <field name="mapslocalizacion"/>
             <field name="localizacionamigable"/>
             <field name="rasa_userid" invisible="1"/>
           </group>
		   <group string="Actividad y Horarios">
             <field name="horarioc"/>
             <field name="actividad"/>

           </group>
		   </group>

        </page>
        </xpath>
      </field>
    </record>

  <record id="view_res_partner_inherit_extra_data2_form" model="ir.ui.view">
    <field name="name">view.res.partner.inherit.extra.data2.form</field>
    <field name="model">res.partner</field>
    <field name="inherit_id" ref="base.view_partner_form"/>
    <field name="arch" type="xml">
      <field name="function" position="after">

      <field name="rasa_telegramid" string="Telegram ID"/>

      </field>
    </field>
  </record>
  
  

  
  
