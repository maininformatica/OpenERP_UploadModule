					<field name="idconfnotif"/>
					<field name="user_ids" widget="many2many_tags"/>
					<field name="partner_ids" widget="many2many_tags"/>
					<field name="enviado"/>
					<field name="enviado_cuando"/>
					<field name="pendiente" readonly="1"/>
